{
  "productName": "Casino Vouchers Pure",
  "appId": "com.coralreef.casinovouchers.pure",
  "directories": {
    "output": "dist"
  },
  "files": [
    "pure/**/*",
    "package.json",
    "node_modules/**/*",
    "!node_modules/**/test/**",
    "!node_modules/**/*.md",
    "!**/*.map"
  ],
  "extraMetadata": {
    "main": "pure/main.js"
  },
  "win": {
    "target": ["portable"],
    "requestedExecutionLevel": "asInvoker"
  },
  "portable": {
    "artifactName": "CasinoVouchersPure.exe",
    "requestExecutionLevel": "user",
    "unpackDirName": "casino-data"
  },
  "compression": "maximum"
}
